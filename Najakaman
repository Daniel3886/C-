#include <iostream>

int main() {
    const int rows = 5;
    const int cols = 5;

    char tablica[rows][cols] = {
        {'9', '1', '1', '1', '1'},
        {'1', '9', '1', '1', '1'},
        {'1', '1', '9', '1', '1'},
        {'1', '1', '1', '9', '1'},
        {'1', '1', '1', '1', '9'}
    };

    // Wydrukuj tablicÄ™ wierszami
    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < cols; ++j) {
            std::cout << tablica[i][j];
        }
        std::cout << std::endl;
    }

    return 0;
}
